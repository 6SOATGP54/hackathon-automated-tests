  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "cypress": "^14.0.2",
    "eslint": "^9.9.1",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-cypress": "^3.5.0",
    "globals": "^15.9.0"
  }
}
